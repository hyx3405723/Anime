page{
  padding: 0;
  margin: 0;
}
.history{
  padding-left: 20rpx;
}
navigator{
  padding: 20rpx;
  padding-left: 0rpx;
  display: flex;
  border-bottom: 1rpx solid #eee;
}
navigator image{
  width: 230rpx;
  height: 140rpx;
  border-radius: 15rpx;
  margin-right: 20rpx;
}
.history_content{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  font-size: 28rpx;
}
.history_foot{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  font-size: 26rpx;
  color: rgb(141, 141, 141);
}
.history_foo text{
  margin-bottom: 10rpx;
}